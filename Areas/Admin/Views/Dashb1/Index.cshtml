@model IEnumerable<GestionAbscences.Data.entite>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Dashboard - SB Admin</title>
    <link href="css/styles.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/js/all.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.bundle.min.js"></script>
</head>
<body class="sb-nav-fixed">




    <main>
      

        <div class="container-fluid" style="margin-top:15px;">


            <div class="row">

                <!-- /global -->
                <div class="col-xl-3 col-md-6">
                    <div class="card bg-primary text-white mb-4">
                        <div class="card-body d-flex align-items-center justify-content-between " style="height:20px; font-weight:bold;">
                            <a class=" text-white stretched-link" href="@Url.Action("Global", "DashB1")">Demande de Congé</a>
                            <div class="small text-white"><i class="fas fa-angle-right"></i></div>

                        </div>
                        <div class="card-footer" style="height:70px;">
                            <div style="font-weight:bold;">
                                100%
                            </div>
                            <div>
                                @Session["global"]
                            </div>
                        </div>
                    </div>
                </div>

                <!-- /EnCours-->

                <div class="col-xl-3 col-md-6">
                    <div class="card bg-warning text-white mb-4">
                        <div class="card-body d-flex align-items-center justify-content-between " style="height:20px; font-weight:bold;">
                            <a class=" text-white stretched-link" href="@Url.Action("Encours", "DashB1")">Demande en attente </a>
                            <div class="small text-white"><i class="fas fa-angle-right"></i></div>

                        </div>
                        <div class="card-footer" style="height:70px;">
                            <div style="font-weight:bold;">
                                @Session["enCours1"]%
                            </div>
                            <div>
                                @Session["enCours"]
                            </div>
                        </div>
                    </div>
                </div>

                <!-- /accepte -->

                <div class="col-xl-3 col-md-6">
                    <div class="card bg-success text-white mb-4">
                        <div class="card-body d-flex align-items-center justify-content-between " style="height:20px; font-weight:bold;">
                            <a class=" text-white stretched-link" href="@Url.Action("Accepte", "DashB1")">Demande Accepté </a>
                            <div class="small text-white"><i class="fas fa-angle-right"></i></div>

                        </div>
                        <div class="card-footer" style="height:70px;">
                            <div style="font-weight:bold;">
                                @Session["accept1"]%
                            </div>
                            <div>
                                @Session["accepte"]
                            </div>
                        </div>
                    </div>
                </div>

                <!-- /Refuse-->

                <div class="col-xl-3 col-md-6">
                    <div class="card bg-danger text-white mb-4">
                        <div class="card-body d-flex align-items-center justify-content-between " style="height:20px; font-weight:bold;">
                            <a class=" text-white stretched-link" href="@Url.Action("Refuse", "DashB1")">Demande Refusé </a>
                            <div class="small text-white"><i class="fas fa-angle-right"></i></div>

                        </div>
                        <div class="card-footer" style="height:70px;">
                            <div style="font-weight:bold;">
                                @Session["refuse1"]%
                            </div>
                            <div>
                                @Session["refuse"]
                            </div>
                        </div>
                    </div>
                </div>

                @Session["name"]


                <div class="card card-success col-12">
                    <div class="card-header">
                        <h3 class="card-title">Demande de congé</h3>


                    </div>
                    <div class="card-body ">
                        <div class="chart">
                            <img src="@Url.Action("MyChart" , "Dashb1")">
                        </div>
                    </div>
                    <!-- /.card-body -->
                </div>

                <div class="card card-success col-12">

                    <div class="card-body ">
                        <div class="chart">
                            <img src="@Url.Action("MyLine" , "Dashb1")">
                        </div>
                    </div>
                    <!-- /.card-body -->
                </div>

                <div class="card card-success col-12">

                    <div class="card-body ">
                        <div class="chart">
                            <img src="@Url.Action("Pie1" , "Dashb1")">
                            <img src="@Url.Action("Pie2" , "Dashb1")">

                        </div>
                    </div>
                    <!-- /.card-body -->
                </div>



                <!-- jQuery -->
                <script src="~/Scripts/jquery-3.4.1.min.js"></script>
                <!-- Bootstrap 4 -->
                <!-- ChartJS -->
                <script src="~/Scripts/Chart.min.js"></script>

                <!-- AdminLTE for demo purposes -->
    </main>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="js/scripts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
    <script src="assets/demo/chart-area-demo.js"></script>
    <script src="assets/demo/chart-bar-demo.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js" crossorigin="anonymous"></script>
    <script src="assets/demo/datatables-demo.js"></script>
</body>
</html>


